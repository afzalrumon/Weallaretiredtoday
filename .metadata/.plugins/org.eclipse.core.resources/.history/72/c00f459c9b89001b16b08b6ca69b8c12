package com.class16;

public class Employee {
	int eId;
	double salary;
	static String CEO="Sumair";
	public static void main(String[] args) {
		
	Employee asghar=new Employee();
	asghar.eId=0001;
	asghar.salary=150000;
	
	System.out.println(asghar.eId+asghar.salary+CEO);
	Employee asel=new Employee();
	asel.eId=002;
	asel.salary=150000;
	System.out.println(asel.eId+asel.salary+CEO);
		
		
		
		
		
		
		
		
		
		
		
		
		
		
	}

}
